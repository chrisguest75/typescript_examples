{
  "name": "ffprober",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "clean:build": "npm run rebuild",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "npm run rebuild && nodemon",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:cmd:singlesegment": "npm run rebuild && node ./build/src/index.js --path '/Volumes/videoshare/bigbuckbunny-hls/segment-0.ts' --out './out' --includegop",
    "start:cmd:segments": "npm run rebuild && node ./build/src/index.js --path '/Volumes/videoshare/bigbuckbunny-hls' --out './out' --includegop",
    "start:cmd:cosmos": "npm run rebuild && node ./build/src/index.js --path '/Volumes/videoshare/cosmos' --out './out' --includegop",
    "start:cmd:all": "npm run rebuild && node ./build/src/index.js --path '/Volumes/videoshare' --out './out' --includegop",
    "start:cmd:path": "npm run rebuild && node ./build/src/index.js --path $npm_config_path --out './out' $npm_config_includegop",
    "start:cmd:missing": "npm run rebuild && node ./build/src/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "nodemon.json",
      "tsconfig.json",
      "package.json",
      "package-lock.json",
      ".env"
    ],
    "ext": "ts",
    "ignore": [],
    "exec": "ts-node ./src/index.ts --path '/Volumes/videoshare/cosmos'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/md5-file": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.11",
    "@types/papaparse": "^5.3.1",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.5",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/auto-instrumentations-node": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "dotenv": "^10.0.0",
    "md5-file": "^5.0.0",
    "minimist": "^1.2.5",
    "papaparse": "^5.3.1",
    "pino": "^7.5.1"
  }
}
